{
    "manifest_version": 3,
    "name": "2FA TOTP browser extention",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "main.html"
    },
    "background": {
      "service_worker": "scripts/background.js"
    },
    "permissions": [
        "declarativeNetRequestWithHostAccess",
        "storage"
      ],
      "host_permissions": [ "*://*/*" ]
  }
